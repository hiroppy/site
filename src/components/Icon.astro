---
import { Icon as AstroIcon } from "astro-icon/components";

export type Props = {
  icon: string;
  class?: string;
  width?: string | number;
  height?: string | number;
};

const { icon, class: className, width = "1em", height = "1em" } = Astro.props;
---

<AstroIcon name={icon} class={className} width={width} height={height} />
