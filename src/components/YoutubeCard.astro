---
type Props = {
  id: string;
};

const { id } = Astro.props;
---

<iframe
  width="480"
  height="270"
  src={`https://www.youtube.com/embed/${id}?feature=oembed`}
  frameborder="0"
  allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
  allowfullscreen=""
  id="widget2"></iframe>
